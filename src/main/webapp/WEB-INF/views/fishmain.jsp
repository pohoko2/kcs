<script>
var wingbanner=$('.scroll-banner');
if(wingbanner.length){
	$(document).ready(function(){
		var tt=wingbanner.offset().top - 75;
		$(window).scroll(function() {
			var t = $(this).scrollTop() ;
			if(t>=tt) wingbanner.addClass('fix');
			else wingbanner.removeClass('fix');
		});
	});
}

$(window).bind('scroll', function() {
    if($(window).scrollTop() >= $('.contents').offset().top + $('.contents').outerHeight() - window.innerHeight - 2) {
        $('.btn_scroll-top').removeClass('bix');
    }else{
		$('.btn_scroll-top').addClass('bix');
	}
});


</script>

